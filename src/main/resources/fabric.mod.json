	"authors": [
		"Luxof"
	],
	"contact": {
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/lessertp/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.luxof.lessertp.LesserTeleport",
			{
				"adapter": "kotlin",
				"value": "com.luxof.lessertp.MishapThrower"
			}
		]
	},
	"mixins": [
		"lessertp.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"hexcasting": ">=0.11.2-pre-701"
	}
